<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IsThereClientRelationship">

    <Button
        android:id="@+id/clientRelationshipNotExist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:onClick="clientRelationshipNo"
        android:text="@string/agent_client_relationship_no"
        app:layout_constraintStart_toStartOf="@+id/clientRelationshipExists"
        app:layout_constraintTop_toBottomOf="@+id/clientRelationshipExists" />

    <Button
        android:id="@+id/clientRelationshipExists"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:onClick="clientRelationshipYes"
        android:text="@string/agent_client_relationship_yes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clientRelationshipTitle" />

    <TextView
        android:id="@+id/clientRelationshipTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="191dp"
        android:text="@string/agent_buyer_relationship_exists"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>